  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "ASK", "[ ]", "[X]"],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "tag",
      "gutterIcon": true
    },
    "FIXME": {
      "foreground": "white",
      "background": "DarkRed",
      "opacity": 100,
      "iconColour": "red",
      "gutterIcon": true
    },
    "[ ]": {
      "icon": "tasklist",
      "background": "OrangeRed",
      "gutterIcon": true
    },
    "[X]": {
      "icon": "tasklist",
      "background": "DarkGreen",
      "gutterIcon": true
    },
    "ASK": {
      "icon": "comment-discussion",
      "gutterIcon": true
    },
    "RM": {
      "icon": "trash",
      "background": "red",
      "type": "line",
      "gutterIcon": true
    }
  },
